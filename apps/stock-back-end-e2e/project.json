{
  "name": "stock-back-end-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["stock-back-end"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/stock-back-end-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["stock-back-end:build", "stock-back-end:serve"]
    }
  }
}
